<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.likailing.cloud.service.manager.mapper.CourseHomeworkMapper">

    <select id="selectTeacherHomework" resultType="CourseHomework">
        SELECT *
        FROM course_homework ch,
             manager_timetable mt
        WHERE
            ch.timetable_id = mt.id
          AND mt.teacher_id = #{teacherId}
    </select>

</mapper>
